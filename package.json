{
  "private": true,
  "name": "@eviloma/evilbot",
  "description": "Official discord bot for Eviloma server",
  "version": "4.3.1",
  "homepage": "https://github.com/Eviloma/evilbot",
  "license": "MIT",
  "packageManager": "pnpm@8.10.0",
  "main": "dist/index.js",
  "bugs": {
    "url": "https://github.com/Eviloma/evilbot/issues",
    "email": "support@eviloma.org"
  },
  "author": {
    "name": "HighError",
    "url": "https://github.com/higherror",
    "email": "hierrorua@gmail.com"
  },
  "keywords": [
    "discord, discord-bot, eviloma"
  ],
  "scripts": {
    "clean": "del /s /q dist",
    "dev": "tsc-watch --onCompilationComplete \"tscp\" --onSuccess \"node .\"",
    "build": "tsc --build && tscp",
    "start": "node .",
    "lint": "eslint -f mo .",
    "format": "prettier --write .",
    "prepare": "node .husky/install.mjs",
    "db:generate": "drizzle-kit generate:pg --config=./src/drizzle.config.ts",
    "db:migrate": "node ./dist/migrate.mjs"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "canvacord": "^6.0.1",
    "console-table-printer": "^2.12.0",
    "dayjs": "^1.11.10",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.29.4",
    "easy-discord-components": "^1.1.2",
    "envalid": "^8.0.0",
    "glob": "^10.3.10",
    "kazagumo": "^3.0.1",
    "kazagumo-spotify": "^2.0.1",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.6",
    "plurals-cldr": "^2.0.1",
    "postgres": "^3.4.3",
    "shoukaku": "^4.0.1",
    "winston": "^3.11.0",
    "winston-console-format": "^1.0.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.20",
    "@types/plurals-cldr": "^1.0.5",
    "@types/react": "^18.2.58",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-mo": "^1.2.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-xss": "^0.1.12",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.0.4",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-cp": "^0.1.9"
  }
}
